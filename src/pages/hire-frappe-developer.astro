---
import Layout from "../layouts/Layout.astro";
import "../styles.css";
import { Image, Picture } from "@astrojs/image/components";

import frappe from "../../public/Images/frappe.svg";
import time_zone from "../../public/Images/time_zone.svg";
import blue_tick from "../../public/Images/blue_tick.svg";
import frappe_developer from "../../public/Images/frappe_developer.svg";
import key_skill_1 from "../../public/Images/key_skill_1.svg";
import key_skill_2 from "../../public/Images/key_skill_2.svg";
import key_skill_3 from "../../public/Images/key_skill_3.svg";
import key_skill_4 from "../../public/Images/key_skill_4.svg";
import key_skill_5 from "../../public/Images/key_skill_5.svg";
import key_skill_6 from "../../public/Images/key_skill_6.svg";
import key_skill_7 from "../../public/Images/key_skill_7.svg";
import key_skill_8 from "../../public/Images/key_skill_8.svg";
import frappe_astro from "../../public/Images/frappe_astro.svg";
import frappe_vue from "../../public/Images/frappe_vue.svg";
import right_arrow from "../../public/Images/right-arrow.svg";
import frappe_react from "../../public/Images/frappe_react.svg";
import sanskar_technolab from "../../public/Images/sanskar_technolab.svg";
import frappe_certified_developer from "../../public/Images/frappe_certified_developer.svg";
import Project_Details_Sharing from "../../public/Images/Project_Details_Sharing.svg";
import Assess_Shortlist_Candidate from "../../public/Images/Assess_Shortlist_Candidate.svg";
import Interview_Candidate from "../../public/Images/Interview_Candidate.svg";
import Onboarding_Manage from "../../public/Images/Onboarding_Manage.svg";
import frappe_developer_service from "../../public/Images/frappe_developer_service.png";
import python from "../../public/Images/python.svg";
import frappe_logo from "../../public/Images/frappe_logo.svg";
import github from "../../public/Images/github.svg";
import hire_empower from "../../public/Images/hire_empower.svg";
import down_arrow from "../../public/Images/down_arrow.svg";
import up_arrow from "../../public/Images/up_arrow.svg";
import html from "../../public/Images/html.svg";
import js from "../../public/Images/js.svg";
import astro from "../../public/Images/astro.svg";
import react from "../../public/Images/react.svg";
import vuejs from "../../public/Images/vuejs.svg";
import bootstrap from "../../public/Images/bootstrap.svg";
import tailwindcss from "../../public/Images/tailwindcss.svg";
import mariadb from "../../public/Images/mariadb.svg";
import mysql from "../../public/Images/mysql.svg";
import oracle from "../../public/Images/oracle.svg";
import postgresql from "../../public/Images/postgresql.svg";
import sqlite from "../../public/Images/sqlite.svg";
import aws from "../../public/Images/aws.svg";
import digital_ocean from "../../public/Images/digital_ocean.svg";
import docker from "../../public/Images/docker.svg";
import frappe_cloud from "../../public/Images/frappe_cloud.svg";
import google_cloud from "../../public/Images/google_cloud.svg";
import sanskar_developer from "../../public/Images/sanskar_developer.svg";
import Sanskar_Guarantee_bigger from "../../public/Images/Sanskar_Guarantee_bigger.png";
import Sanskar_Guarantee_small from "../../public/Images/Sanskar_Guarantee_small.svg";
import best_erpnext_developer from "../../public/Images/best_erpnext_developer.svg";
import Certified_Developer from "../../public/Images/Certified_Developer.svg";
import Transparency from "../../public/Images/Transparency.svg";
import Higher_ROI from "../../public/Images/Higher_ROI.svg";
import Timely_Delivery from "../../public/Images/Timely_Delivery.svg";
import Code_Ownership from "../../public/Images/Code_Ownership.svg";
import Privacy_Security from "../../public/Images/Privacy_Security.svg";
--- 

<Layout>
  <!-- Bannner Start -->
  <section
    class=" lg:flex section-bg min-h-[80vh] items-center banner-bg"
  >
    <div
      class="flex items-center  md:max-w-7xl md:mx-auto p-5 sm:p-10 md:p-24 2xl:px-0 relative "
    >
      <div class="flex lg:flex-row flex-col max-lg:gap-5">
        <div class="lg:basis-6/12 flex flex-col justify-center">
          <h1
            class="text-2xl md:text-3xl lg:text-4xl md:text-[2rem] lg:text-[2.5rem] lg:leading-[3rem] text-[#4D4C4D] font-bold"
          >
            Hire Frappe Developer
          </h1>
          <p
            class="text-[#31393C] text-sm md:text-base lg:text-lg font-semibold leading-5 md:leading-6 mt-3 md:mt-7"
          >
            Get top-notch software with our Frappe developers. Your vision, our
            expertise! Transform your ideas into reality with our skilled Frappe
            development team. Benefit from top-notch Frappe development services
            to seamlessly bridge the gap between your requirements and available
            resources.
          </p>

          <div class="lg:mt-5 mt-3 md:mt-7">
            <div class="flex flex-row gap-2">
              <Image loading="eager" src={blue_tick} alt="blue_tick" />
              <p class="text-sm md:text-base font-semibold text-[#31393C]">
                Professional Team of Frappe Programmers
              </p>
            </div>
            <div class="flex flex-row gap-2 mt-2">
              <Image loading="eager" src={blue_tick} alt="blue_tick" />
              <p class="text-sm md:text-base font-semibold text-[#31393C]">
                Save up to 60% of development cost
              </p>
            </div>
            <div class="flex flex-row gap-2 mt-2">
              <Image loading="eager" src={blue_tick} alt="blue_tick" />
              <p class="text-sm md:text-base font-semibold text-[#31393C]">
                Top 1% of Frappe developers
              </p>
            </div>
            <div class="flex flex-row gap-2 mt-2">
              <Image loading="eager" src={blue_tick} alt="blue_tick" />
              <p class="text-sm md:text-base font-semibold text-[#31393C]">
                Regular Reports and Communication
              </p>
            </div>
          </div>

          <div class=" mt-5 lg:mt-7  flex flex-row gap-5">
            <div class="flex flex-col sm:flex-row border-[#D9E3EC] max-w-[185px] sm:gap-3">
              <Image loading="eager" src={frappe} alt="frappe" />
              <h4 class="text-[#31393C] text-sm md:text-base mt-2 font-medium">
                Certified Frappe Developer
              </h4>
            </div>
            <div class="flex flex-col sm:flex-row border-[#D9E3EC] max-w-[185px] sm:gap-3">
              <Image loading="eager" src={time_zone} alt="time_zone" />
              <h4 class="text-[#31393C] text-sm md:text-base mt-2 font-medium">
                Time Zone Aligned
              </h4>
            </div>
          </div>



        </div>
        <!-- form  start -->
        <div
          id="schedule-interview"
          class="interview rounded-[20px] shadow-lg lg:ml-10"
        >
          <form id="frappe_hire_form">
            <div class="2xl:py-6 2xl:px-8 py-5 xl:py-5 px-5">
              <p
                class="text-[#3E96F4] text-lg md:text-xl lg:text-3xl font-bold"
              >
                Schedule A Developer Interview
              </p>

              <p
                class="text-[#31393C] text-sm md:text-base lg:text-lg font-semibold mt-2 2xl:mt-3"
              >
              We ensure youâ€™re matched with the right talent based on your requirement.
              </p>

              <div class="grid grid-cols-1 2xl:mt-6 lg:mt-5 mt-3">
                <div>
                  <input
                    type="text"
                    placeholder=" Name*"
                    class="rounded w-full p-2 border border-[#D9D9D9]"
                    name="name"
                    required
                  />
                </div>

                <div class="mt-3 2xl:mt-5">
                  <input
                    id="email"
                    type="email"
                    placeholder=" Email*"
                    class="rounded w-full p-2 border border-[#D9D9D9]"
                    name="email"
                    required
                  />
                </div>

                <div class="mt-3 2xl:mt-5">
                  <input
                    type="tel"
                    placeholder=" Phone Number"
                    class="rounded w-full p-2 border border-[#D9D9D9]"
                    name="phone_no"
                  />
                </div>
              </div>

              <div class="mt-3 2xl:mt-5">
                <textarea
                  name="req_message"
                  id=""
                  rows="5"
                  placeholder="Describe Your Requirements*"
                  class="w-full rounded p-3 border border-[#D9D9D9]"
                  required></textarea>
              </div>

              <div class="mt-5 md:mt-7 lg:mt-3 2xl:mt-5">
                <button
                  class="text-[#FFFFFF] bg-[#3E96F4] rounded font-semibold text-base font-inter px-5 py-2"
                  type="submit">Schedule Interview</button
                >
              </div>
            </div>
          </form>
        </div>
        <!-- form end -->
      </div>
    </div>
  </section>
 <!-- Bannner End-->

  <!-- Top Frappe Development Company start -->
  <section
    class="mx-5 md:max-w-7xl md:mx-auto md:px-24 2xl:px-0 py-[2rem] md:py-[2rem] lg:py-[4rem]"
  >
    <h2
      class="text-[#3E96F4] text-2xl md:text-3xl lg:text-4xl font-semibold text-center"
    >
      Top Frappe Development Company
    </h2>
    <div
      class="flex gap-5 lg:gap-10 lg:flex-row flex-col max-lg:items-center justify-evenly mt-5 lg:mt-10"
    >
      <div class="max-lg:mx-auto">
        <Image
          class="max-sm:h-[250px]"
          loading="eager"
          src={frappe_developer}
          alt="frappe_developer"
        />
      </div>
      <div class="w-full lg:w-1/2">
        <p class="text-[#31393C] text-xs md:text-sm lg:text-base">
          As a premier Frappe development company, Sanskar Technolab stands out
          as your ultimate destination for hiring skilled Frappe developers. As
          a Frappe-certified company, we bring unparalleled expertise and
          credibility to every project we undertake. Specializing in both
          in-house and offshore talent, we excel in crafting cutting-edge
          applications tailored to the unique needs of enterprises across
          diverse industry verticals.
        </p>
        <p class="text-[#31393C] text-xs md:text-sm lg:text-base mt-5">
          At the core of our offerings lies a team of seasoned developers adept
          at crafting high-end Frappe applications. Drawing from extensive
          experience, our developers have a proven track record of delivering
          solutions that drive tangible business growth for our clients.
        </p>
        <p class="text-[#31393C] text-xs md:text-sm lg:text-base mt-5">
          Our developers possess a deep understanding of Frappe development and
          full-stack procedures, enabling them to seamlessly navigate the
          complexities of application development. From conceptualization to
          deployment, we leave no stone unturned in ensuring that our solutions
          are not just functional, but exceptional.
        </p>
        <p class="text-[#31393C] text-xs md:text-sm lg:text-base mt-5">
          Partner with us to unlock the true potential of Frappe and propel your
          business towards digital transformation and sustained growth.
        </p>
      </div>
    </div>
  </section>
  <!-- Top Frappe Development Company End -->

  <!-- Our Frappe Developement Services start -->
  <section
    class="w-full md:px-24 2xl:px-0 max-lg:py-[3rem] lg:py-[4rem] bg-[#326EB0]"
  >
    <div
      class="flex flex-col lg:flex-row max-lg:items-center gap-10 mx-5 md:max-w-7xl md:mx-auto md:px-24 2xl:px-0"
    >
      <div class=" lg:w-1/2">
        <h2
          class="text-2xl md:text-3xl lg:text-4xl font-semibold text-[#FFFFFF] max-w-[450px]"
        >
          Our Frappe Developement Services
        </h2>
        <div
          class=" bg-gradient-to-br from-[#E0EFFF] to-[#A3D0FF] mt-5 lg:mt-7 p-3 lg:p-6 rounded-xl border-[#FFFFFF] border-2 max-w-[540px]"
        >
          <h2 class="text-[#31393C] text-lg md:text-xl font-bold">
            Frappe Full Stack Development
          </h2>
          <p class="text-sm md:text-base text-[#31393C] mt-3">
            Frappe Full Stack Development Services offer a flexible approach,
            seamlessly combining Frappe's robust Python backend with front-end
            technologies like React, Vue, and Astro. This ensures efficient
            development, adherence to coding standards, and the creation of
            dynamic, scalable solutions tailored to your specific needs.
          </p>
        </div>
        <div
          class="bg-gradient-to-br from-[#E0EFFF] to-[#A3D0FF] mt-5 p-3 lg:p-6 rounded-xl border-[#FFFFFF] border-2 max-w-[540px]"
        >
          <h2 class="text-[#31393C] text-lg md:text-xl font-bold">
            Software Product Development
          </h2>
          <p class="text-sm md:text-base text-[#31393C] mt-3">
            Product engineering services smoothly blend product design and
            development using the versatile Frappe Framework. This method
            incorporates various technologies like software, IT services, and
            systems throughout the product development journey. From
            brainstorming and innovation to design, development, and rollout,
            the Frappe Framework boosts efficiency and effectiveness at every
            step.
          </p>
        </div>
        <div
          class="bg-gradient-to-br from-[#E0EFFF] to-[#A3D0FF] mt-5 p-3 lg:p-6 rounded-xl border-[#FFFFFF] border-2 max-w-[540px]"
        >
          <h2 class="text-[#31393C] text-lg md:text-xl font-bold">
            Frappe App Development
          </h2>
          <p class="text-sm md:text-base text-[#31393C] mt-3">
            We specialize in crafting secure and highly scalable web
            applications using the Frappe framework, empowering organizations to
            boost productivity and streamline operations. With a keen focus on
            robust security features, our solutions ensure data integrity and
            user trust, while harnessing Frappe's flexibility for seamless
            scalability and future growth.
          </p>
        </div>
        <div
          class="bg-gradient-to-br from-[#E0EFFF] to-[#A3D0FF] mt-5 p-3 lg:p-6 rounded-xl border-[#FFFFFF] border-2 max-w-[540px]"
        >
          <h2 class="text-[#31393C] text-lg md:text-xl font-bold">
            API Development / Custom Module Development
          </h2>
          <p class="text-sm md:text-base text-[#31393C] mt-3">
            We specialise in developing well-documented, stable, and secure
            Frappe APIs tailored to power up your mobile and web applications.
            With the expertise of our Frappe developers, we craft APIs for
            seamless communication with third-party devices, delivering robust
            and Restful solutions for your needs.
          </p>
        </div>

        <div
          class="bg-gradient-to-br from-[#E0EFFF] to-[#A3D0FF] mt-5 p-3 lg:p-6 rounded-xl border-[#FFFFFF] border-2 max-w-[540px]"
        >
          <h2 class="text-[#31393C] text-lg md:text-xl font-bold">
            Frappe App/Product Support and Maintenance
          </h2>
          <p class="text-sm md:text-base text-[#31393C] mt-3">
            Ensure the smooth operation of your Frappe-based applications with
            our reliable support and maintenance services. From troubleshooting
            issues to implementing upgrades, we keep your systems running
            optimally.
          </p>
        </div>
      </div>

      <div class= "w-1/2 max-lg:hidden">
        <Image
          class="max-lg:hidden fixed-image w-full"
          loading="eager"
          src={frappe_developer_service}
          alt="frappe_developer_service"
        />
      </div>
    </div>
  </section>
  <!-- Our Frappe Developement Services End -->

  <!-- Key Skills of our Frappe Developer start -->
  <section
    class="mx-5 md:max-w-7xl md:mx-auto md:px-24 2xl:px-0 py-[2rem] md:py-[2rem] lg:py-[4rem]"
  >
    <div>
      <h2
        class="text-[#3E96F4] text-2xl md:text-3xl lg:text-4xl font-semibold text-center"
      >
        Key Skills of our Frappe Developer
      </h2>
      <div class="grid grid-cols-1 lg:grid-cols-2 mt-5 md:mt-7 lg:mt-10 gap-5">
        <div
          class="drop-shadow-lg flex flex-col sm:flex-row lg:items-center gap-4 bg-gradient-to-br from-[#ECF5FF] to-[#F6FAFF] max-lg:mx-auto p-4 rounded-xl border-[#FFFFFF] border-2"
        >
          <Image loading="eager" src={key_skill_1} alt="key_skill_1" />
          <p class="text-sm md:text-base">
            Professionals proficient in Frappe's latest version 15, adept at
            crafting efficient solutions that are straightforward to deploy,
            manage, and troubleshoot.
          </p>
        </div>
        <div
          class="drop-shadow-lg flex lg:items-center flex-col sm:flex-row gap-4 bg-gradient-to-br from-[#ECF5FF] to-[#F6FAFF] max-lg:mx-auto p-4 rounded-xl border-[#FFFFFF] border-2"
        >
          <Image loading="eager" src={key_skill_5} alt="key_skill_5" />
          <p class="text-sm md:text-base">
            Developing applications using the Frappe framework, including
            understanding how to create and customize DocTypes, Controllers,
            Views, and Routes.
          </p>
        </div>
        <div
          class="drop-shadow-lg lg:items-center flex flex-col sm:flex-row gap-4 bg-gradient-to-br from-[#ECF5FF] to-[#F6FAFF] max-lg:mx-auto p-4 rounded-xl border-[#FFFFFF] border-2"
        >
          <Image loading="eager" src={key_skill_2} alt="key_skill_2" />
          <p class="text-sm md:text-base">
            Proficient in server-side and client-side scripting, crafting
            interactive dashboards and reports, executing migrations, and
            providing end-to-end maintenance.
          </p>
        </div>

        <div
          class="drop-shadow-lg lg:items-center flex flex-col sm:flex-row gap-4 bg-gradient-to-br from-[#ECF5FF] to-[#F6FAFF] max-lg:mx-auto p-4 rounded-xl border-[#FFFFFF] border-2"
        >
          <Image loading="eager" src={key_skill_6} alt="key_skill_6" />
          <p class="text-sm md:text-base">
            Excellent in SQL and database concepts, particularly in
            MariaDB/MySQL, for efficient data storage and retrieval in Frappe
            applications.
          </p>
        </div>

        <div
          class="drop-shadow-lg flex flex-col sm:flex-row gap-4 lg:items-center bg-gradient-to-br from-[#ECF5FF] to-[#F6FAFF] max-lg:mx-auto p-4 rounded-xl border-[#FFFFFF] border-2"
        >
          <Image loading="eager" src={key_skill_3} alt="key_skill_3" />
          <p class="text-sm md:text-base">
            Our Frappe Developers have strong problem-solving skills to debug
            issues, optimize performance, and implement new features
            effectively.
          </p>
        </div>
        <div
          class="drop-shadow-lg flex flex-col sm:flex-row gap-4 lg:items-center bg-gradient-to-br from-[#ECF5FF] to-[#F6FAFF] max-lg:mx-auto p-4 rounded-xl border-[#FFFFFF] border-2"
        >
          <Image loading="eager" src={key_skill_7} alt="key_skill_7" />
          <p class="text-sm md:text-base">
            The ability to document code, APIs, and development processes is
            important for maintaining code quality and collaborating with other
            developers.
          </p>
        </div>

        <div
          class="drop-shadow-lg flex flex-col sm:flex-row gap-4 lg:items-center bg-gradient-to-br from-[#ECF5FF] to-[#F6FAFF] max-lg:mx-auto p-4 rounded-xl border-[#FFFFFF] border-2"
        >
          <Image loading="eager" src={key_skill_4} alt="key_skill_4" />
          <p class="text-sm md:text-base">
            Proficiency in designing and consuming REST APIs for seamless
            integration with external systems.
          </p>
        </div>

        <div
          class="drop-shadow-lg flex flex-col sm:flex-row lg:items-center gap-4 bg-gradient-to-br from-[#ECF5FF] to-[#F6FAFF] max-lg:mx-auto p-4 rounded-xl border-[#FFFFFF] border-2"
        >
          <Image loading="eager" src={key_skill_8} alt="key_skill_8" />
          <p class="text-sm md:text-base">
            Strong problem-solving skills to debug issues, optimize performance,
            and implement new features effectively.
          </p>
        </div>
      </div>
    </div>
    <div class="2xl:px-0 pt-[2rem] md:pt-[2rem] lg:pt-[4rem]">
      <h2
        class="text-[#3E96F4] text-2xl md:text-3xl lg:text-4xl font-semibold text-center"
      >
        Most Preferred Frappe Combinations We Offer
      </h2>
      <div
        class="grid grid-cols-1 lg:grid-rows-1 lg:grid-cols-3 mt-5 md:mt-7 lg:mt-10 gap-5"
      >
        <div
          class="drop-shadow-lg max-lg:mx-auto p-5 rounded-xl border-[#CCE4FF] bg-[#FFFFFF] border-2"
        >
          <Image loading="eager" src={frappe_react} alt="frappe_react" />
          <p class="text-[#31393C] sm:text-base md:text-lg font-semibold mt-3">
            Frappe + React
          </p>
          <p class="text-sm md:text-base text-[#31393C] mt-3">
            Frappe and React work well together. Frappe makes it easy to connect
            with other tools, while React helps manage different software parts
            smoothly. So, when they team up, developers can build apps that work
            seamlessly across different devices and systems, making life easier
            for users.
          </p>
        </div>
        <div
          class="drop-shadow-lg max-lg:mx-auto p-5 rounded-xl border-[#CCE4FF] bg-[#FFFFFF] border-2"
        >
          <Image loading="eager" src={frappe_vue} alt="frappe_vue" />
          <p class="text-[#31393C] sm:text-base md:text-lg font-semibold mt-3">
            Frappe + Vue
          </p>
          <p class="text-sm md:text-base text-[#31393C] mt-3">
            The partnership of Vue.js and Frappe offers a powerful solution for
            modern web application development. Their seamless integration
            promises a smooth and efficient development process, resulting in
            exceptional user interfaces supported by a strong backend framework.
          </p>
        </div>
        <div
          class="drop-shadow-lg max-lg:mx-auto p-5 rounded-xl border-[#CCE4FF] bg-[#FFFFFF] border-2"
        >
          <Image loading="eager" src={frappe_astro} alt="frappe_astro" />
          <p class="text-[#31393C] sm:text-base md:text-lg font-semibold mt-3">
            Frappe + Astro
          </p>
          <p class="text-sm md:text-base text-[#31393C] mt-3">
            Frappe and Astro team up for powerful web development. Frappe's
            backend features blend seamlessly with Astro's frontend toolkit,
            facilitating smooth integration for building modern web apps.
            Together, they enable developers to create dynamic, feature-rich
            interfaces backed by a robust backend framework.
          </p>
        </div>
      </div>
    </div>
  </section>
  <!-- Key Skills of our Frappe Developer End -->

  <!-- Hiring Process End -->
  <section class="py-[2rem] md:py-[2rem] lg:py-[4rem] bg-[#FAFAFA]">
    <div
      class="text-[#31393C] mx-5 md:max-w-7xl md:mx-auto md:px-24 2xl:px-0 "
    >
      <h2 class="text-2xl md:text-3xl lg:text-4xl font-semibold text-center">
        Hiring Process
      </h2>
      <div
        class="grid grid-cols-1 lg:grid-rows-1 md:grid-cols-2 lg:grid-cols-4 mt-5 md:mt-7 lg:mt-10 gap-5 lg:gap-8 text-center"
      >
        <div
          class="flex flex-col items-center lg:justify-between max-lg:max-w-[355px] max-w-[250px] max-lg:mx-auto gap-3"
        >
          <Image
            loading="eager"
            src={Project_Details_Sharing}
            alt="Project_Details_Sharing"
          />
          <p class="font-bold text-lg md:text-xl">Project Details Sharing</p>
          <p class="text-sm md:text-base">
            Share the specific requirements of your project, goals, detailing
            its objectives, and the desired profile of the developer you wish to
            hire.
          </p>
        </div>
        <div
          class="flex flex-col items-center lg:justify-between max-lg:max-w-[355px] max-w-[250px] max-lg:mx-auto gap-3"
        >
          <Image
            loading="eager"
            src={Assess_Shortlist_Candidate}
