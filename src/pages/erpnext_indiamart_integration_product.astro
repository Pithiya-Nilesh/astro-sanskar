<script>
    var popupTriggerone = document.getElementById("popup-trigger-one");
    var popupTriggertwo = document.getElementById("popup-trigger-two");
    var popupContainerOne = document.getElementById("popup-container-one");
    var popupContainerTwo = document.getElementById("popup-container-two");

    // Add click event to trigger
    popupTriggerone.addEventListener(
        "click",
        openPopup.bind(null, popupContainerOne)
    );
    popupTriggertwo.addEventListener(
        "click",
        openPopup.bind(null, popupContainerTwo)
    );

    // Function to open the popup
    function openPopup(popupContainer) {
        popupContainer.style.display = "flex";
    }

    // Function to close the popup
    function closePopup(popupContainer) {
        popupContainer.style.display = "none";
    }

    // Close the popup if clicked outside the image
    document.addEventListener("click", function (event) {
        var popupContainers = [popupContainerOne, popupContainerTwo];
        popupContainers.forEach(function (popupContainer) {
            if (event.target === popupContainer) {
                closePopup(popupContainer);
            }
        });
    });

    // Close the popup when the close button is clicked
    var closeBtns = document.querySelectorAll(".close-btn");
    closeBtns.forEach(function (closeBtn) {
        closeBtn.addEventListener("click", function (event) {
            event.stopPropagation();
            var popupContainer = closeBtn.closest(".popup-container");
            closePopup(popupContainer);
        });
    });
</script>
