---
import Layout from "../layouts/Layout.astro";
import "../styles.css";
import { Image, Picture } from "@astrojs/image/components";

import indiamart_img from "../../public/Images/indiamart_img.png";
import erpnext_indiamart from "../../public/Images/erpnext_indiamart_img.png";
import Sales from "../../public/Images/Sales.png";
import five_min from "../../public/Images/five_min.svg";
import hand_make from "../../public/Images/hand_make.svg";
import snap from "../../public/Images/snap.svg";
import dashboard_insights from "../../public/Images/dashboard_insights.svg";
import trends_analysis from "../../public/Images/trends_analysis.svg";
import history_fetching from "../../public/Images/history_fetching.svg";
import fetch_inquiries from "../../public/Images/fetch_inquiries.svg";
import automated_inquiry from "../../public/Images/automated_inquiry.svg";
import generate_forms from "../../public/Images/generate_forms.svg";
import indiamart_key from "../../public/Images/indiamart_key.png";
import indiamart_key_img from "../../public/Images/indiamart_key_img.png";
import authorization_key from "../../public/Images/authorization_key.svg";
import authorization_key_img from "../../public/Images/authorization_key_img.png";
import down_arrow from "../../public/Images/down_arrow.svg";
import up_arrow from "../../public/Images/up_arrow.svg";
---

<Layout
    title="ERPNext Indiamart Integration Product | Sanskar Technolab"
    ,
    keyword="ERPNext partner, ERPNext development, ERP software development, ERPNext implementation services, ERPNext implementation partner in India, ERPNext development in India, ERP consulting services, ERP deployment services, ERP software development company, ERPNext Integration services,	ERPNext implementation partners, ERPNext implementation, erp implementation partner, erp implementation companies, ERPNext software development company, Hire ERPNext developers, ERPNext development services , Erpnext solution company, Frappe ERPNext , Erpnext software , ERPNext open-source, ServiceNow company, ServiceNow implementation partner, Servicenow implementation services, Servicenow implementation solutions"
    ,
    description="We are among the leading ERPNext development companies offering ERPNext solutions to scale & succeed."
    ,
    canonical_href="https://sanskartechnolab.com/erpnext_indiamart_integration_product"
>
    <!-- banner for lg screen start -->
    <section
        class="hidden lg:flex section-bg h-[90vh] xl:h-[75vh] flex items-center banner-bg"
    >
        <div
            class="flex flex-wrap items-center mx-5 md:max-w-7xl md:mx-auto md:px-24 2xl:px-0"
        >
            <div class="w-full md:w-6/12">
                <h1
                    class="text-[1.75rem] md:text-[3rem] leading-9 md:leading-[3.5rem] text-[#31393C] font-semibold"
                >
                    Leads on Autopilot: Switch to Automated Benefits with
                

                <Image
                    loading="eager"
                    src={erpnext_indiamart}
                    alt="erpnext_indiamart"
                    class=""
                />
                
                    Integration!
                </h1>
                <p
                    class="text-[#31393C] text-sm md:text-base leading-5 md:leading-6 mt-7"
                >
                    Ever feel the stress of juggling leads and inquiries from
                    IndiaMart & worried about missing out on potential business?
                </p>
               
                <p
                    class="text[#31393C] text-sm md:text-base leading-5 md:leading-6 mt-0"
                >
                    Worry not!
                </p>
            </div>

            <Image
                loading="eager"
                src={indiamart_img}
                alt="indiamart_integration"
                class="w-full md:w-6/12 lg:p-10 md:p-8"
            />
        </div>
    </section>
    <!-- banner for lg screen end -->

    <!-- banner for sm & md screen start -->
    <section class="section-bg flex items-center md:flex lg:hidden banner-bg">
        <div
            class="flex flex-wrap items-center mx-5 gap-5 md:max-w-7xl md:mx-auto md:px-24 2xl:px-0"
        >
            <div class="w-full lg:w-6/12 mt-5">
                <div
                    class="text-[1.5rem] md:text-[2.25rem] leading-9 md:leading-[3.5rem] text-[#31393C] font-semibold"
                >
                    Leads on Autopilot: Switch to Automated Benefits with
                </div>
                <Image
                    loading="eager"
                    src={erpnext_indiamart}
                    alt="erpnext_indiamart"
                    class=""
                />
                <div
                    class="text-[1.75rem] md:text-[2.25rem] leading-9 md:leading-[3.5rem] text-[#31393C] font-semibold mt-1"
                >
                    Integration!
                </div>
                <p
                    class="text[#31393C] text-sm md:text-base leading-5 md:leading-6 mt-5"
                >
                    Ever feel the stress of juggling leads and inquiries from
                    IndiaMart & worried about missing out on potential business?
                </p>
              
                <p class="md:text-[18px] text-[14px] text-[#31393C]">
                    Worry not!
                </p>
            </div>

            <Image
                loading="eager"
                src={indiamart_img}
                alt="indiamart_integration"
                class="w-full mb-5"
            />
        </div>
    </section>
    <!-- banner for sm & md screen end -->

    <!-- ERPNext-Indiamart Integration start -->
    <section class="section-bg-left_shadow">
        <section
            class="mx-5 md:max-w-7xl md:mx-auto md:px-24 2xl:px-0 py-[2rem] md:py-[2rem] lg:py-[4rem]"
        >
            <h2
                class="mx-auto lg:w-[60rem] text-center text-[#3E96F4] text-2xl md:text-3xl lg:text-4xl font-semibold"
            >
                Introducing Sanskar Technolab's game-changing ERPNext-Indiamart
                Integration.
            </h2>

            <div class="grid grid-cols-1 lg:grid-cols-2 lg:space-x-5 mt-10">
                <div>
                    <Image loading="eager" src={Sales} alt="Sales" class="" />
                </div>

                <div class="mt-7 space-y-7">
                    <div
                        class="card flex p-3 md:p-5 gap-x-3 md:gap-x-5 lg:mr-10"
                    >
                        <Image
                            loading="eager"
                            src={five_min}
                            alt="five_min"
                            class="h-[45px] md:h-[56px] w-[45px] md:w-[56px]"
                        />
                        <p
                            class="text-[#31393C] text-xs md:text-sm lg:text-base"
                        >
                            Imagine this - within every 5 minutes, your
                            IndiaMart inquiries seamlessly sync with your
                            ERPNext system, with no manual hassle.
                        </p>
                    </div>

                    <div
                        class="card flex p-3 md:p-5 gap-x-3 md:gap-x-5 lg:mr-10"
                    >
                        <Image
                            loading="eager"
                            src={hand_make}
                            alt="hand_make"
                            class="h-[45px] md:h-[56px] w-[45px] md:w-[56px]"
                        />
                        <p
                            class="text-[#31393C] text-xs md:text-sm lg:text-base"
                        >
                            It's like having a virtual assistant dedicated to
                            lead management! Say goodbye to lead loss fears, as
                            our feature-packed system simplifies the entire
                            process, making lead tracking and conversions a
                            breeze.
                        </p>
                    </div>

                    <div
                        class="card flex p-3 md:p-5 gap-x-3 md:gap-x-5 lg:mr-10"
                    >
                        <Image
                            loading="eager"
                            src={snap}
                            alt="snap"
                            class="h-[45px] md:h-[56px] w-[45px] md:w-[56px]"
                        />
                        <p
                            class="text-[#31393C] text-xs md:text-sm lg:text-base"
                        >
                            Boost your business with Sanskar Technolab's
                            ERPNext-Indiamart Integration â€“ where leads flow in,
                            and conversions flow out effortlessly!
                        </p>
                    </div>
                </div>
            </div>
        </section>
    </section>
    <!-- ERPNext-Indiamart Integration end -->

    <!-- Features Unveiled start -->
    <section class="section-bg-right">
        <section
            class="mx-5 md:max-w-7xl md:mx-auto md:px-24 2xl:px-0 py-[2rem] md:py-[2rem] lg:py-[4rem]"
        >
            <h2
                class="mx-auto lg:w-[50rem] text-center text-[#3E96F4] text-2xl md:text-3xl lg:text-4xl font-semibold"
            >
                ERPNext-IndiaMart Integration Product Features Unveiled!
            </h2>

            <div class="grid grid-cols-1 lg:grid-cols-2 mt-10 gap-7">
                <div class="features_card flex p-3 md:p-5 gap-x-3 md:gap-x-5">
                    <Image
                        loading="eager"
                        src={dashboard_insights}
                        alt="dashboard_insights"
                        class="h-[50px] md:h-[64px] w-[50px] md:w-[64px]"
                    />
                    <div class="text-[#31393C] text-xs md:text-sm lg:text-base">
                        <p
                            class="font-semibold text-[16px] md:text-[18px] lg:text-[20px]"
                        >
                            Comprehensive Dashboard Insights:
                        </p>
                        <p class="mt-2">
                            Gain end-to-end tracking and make data-driven
                            decisions effortlessly with our comprehensive
                            dashboard.
                        </p>

                        <ul class="list-disc ml-7 mt-2">
                            <li>
                                Time-wise tracking: Daily, weekly, monthly, and
                                overall inquiry count.
                            </li>
                            <li>
                                Status-wise tracking: Open, converted, repeat,
                                do not contact, and not interested inquiries.
                            </li>
                        </ul>
                    </div>
                </div>

                <div class="features_card flex p-3 md:p-5 gap-x-3 md:gap-x-5">
                    <Image
                        loading="eager"
                        src={trends_analysis}
                        alt="trends_analysis"
                        class="h-[50px] md:h-[64px] w-[50px] md:w-[64px]"
                    />
                    <div class="text-[#31393C] text-xs md:text-sm lg:text-base">
                        <p
                            class="font-semibold text-[16px] md:text-[18px] lg:text-[20px]"
                        >
                            Trends and Analysis:
                        </p>
                        <p class="mt-2">
                            Visualize inquiry trends and types with dynamic
                            graphical representations for informed
                            decision-making.
                        </p>

                        <ul class="list-disc ml-7 mt-2">
                            <li>
                                Gain end-to-end insights about the leads and
                                their effective comparison and overview through
                                graphical representation.
                            </li>
                            <li>
                                Analyze trends for strategic planning and make
                                result-driven decisions.
                            </li>
                        </ul>
                    </div>
                </div>

                <div class="features_card flex p-3 md:p-5 gap-x-3 md:gap-x-5">
                    <Image
                        loading="eager"
                        src={history_fetching}
                        alt="history_fetching"
                        class="h-[50px] md:h-[64px] w-[50px] md:w-[64px]"
                    />
                    <div class="text-[#31393C] text-xs md:text-sm lg:text-base">
                        <p
                            class="font-semibold text-[16px] md:text-[18px] lg:text-[20px]"
                        >
                            Past History Fetching:
                        </p>
                        <p class="mt-2">
                            Retrieve all past leads from Indiamart to ERPNext
                            seamlessly, ensuring a complete historical record.
                        </p>

                        <ul class="list-disc ml-7 mt-2">
                            <li>
                                Fetch historical data effortlessly & ensure a
                                comprehensive overview of past leads
                            </li>
                            <li>
                                Add the start and end date(of 7 days) and you
                                will get the details of leads generated within
                                that particular time frame.
                            </li>
                        </ul>
                    </div>
                </div>

                <div class="features_card flex p-3 md:p-5 gap-x-3 md:gap-x-5">
                    <Image
                        loading="eager"
                        src={fetch_inquiries}
                        alt="fetch_inquiries"
                        class="h-[50px] md:h-[64px] w-[50px] md:w-[64px]"
                    />
                    <div class="text-[#31393C] text-xs md:text-sm lg:text-base">
                        <p
                            class="font-semibold text-[16px] md:text-[18px] lg:text-[20px]"
                        >
                            Fetch Inquiries with Customizable Filters:
                        </p>
                        <p class="mt-2">
                            You can easily search and sort leads with filters
                            like name, status, contact info, and more with
                            customized filters.
                        </p>

                        <ul class="list-disc ml-7 mt-2">
                            <li>
                                Efficient data retrieval with customized
                                filtering
                            </li>
                            <li>
                                Fetch inquiries on the go based on specific
                                criteria
                            </li>
                        </ul>
                    </div>
                </div>

                <div class="features_card flex p-3 md:p-5 gap-x-3 md:gap-x-5">
                    <Image
                        loading="eager"
                        src={automated_inquiry}
                        alt="automated_inquiry"
                        class="h-[50px] md:h-[64px] w-[50px] md:w-[64px]"
                    />
                    <div class="text-[#31393C] text-xs md:text-sm lg:text-base">
                        <p
                            class="font-semibold text-[16px] md:text-[18px] lg:text-[20px]"
                        >
                            Automated Inquiry Type Updates:
                        </p>
                        <p class="mt-2">
                            Experience automated updates to the inquiry type
                            based on customer behavior, ensuring accurate
                            categorization.
                        </p>

                        <ul class="list-disc ml-7 mt-2">
                            <li>Streamline inquiry-type updates</li>
                            <li>Enhance accuracy through automation</li>
                            <li>Real-time lead updated in every 5 minutes</li>
                        </ul>
                    </div>
                </div>

                <div class="features_card flex p-3 md:p-5 gap-x-3 md:gap-x-5">
                    <Image
                        loading="eager"
                        src={generate_forms}
                        alt="generate_forms"
                        class="h-[50px] md:h-[64px] w-[50px] md:w-[64px]"
                    />
                    <div class="text-[#31393C] text-xs md:text-sm lg:text-base">
                        <p
                            class="font-semibold text-[16px] md:text-[18px] lg:text-[20px]"
                        >
                            Generate Comprehensive Lead Forms:
                        </p>
                        <p class="mt-2">
                            Create detailed lead forms instantly post-lead
                            conversion, ensuring all necessary details are
                            captured and effective management is possible.
                        </p>

                        <ul class="list-disc ml-7 mt-2">
                            <li>
                                Generate lead forms seamlessly & capture
                                comprehensive lead information
                            </li>
                            <li>
                                Effortlessly view and manage all inquiries in
                                Indiamart with user-friendly navigation
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </section>
    </section>
    <!-- Features Unveiled start -->

    <!-- The Ultimate Guide start -->
    <section class="section-bg-center">
        <section
            class="grid justify-items-center mx-5 md:max-w-7xl md:mx-auto md:px-24 2xl:px-0 py-[2rem] md:py-[2rem] lg:py-[4rem]"
        >
            <h2
                class="mx-auto lg:w-[55rem] text-center text-[#3E96F4] text-2xl md:text-3xl lg:text-4xl font-semibold"
            >
                The Ultimate Guide You Need to Integrate ERPNext with Indimart:
                Getting Started!
            </h2>

            <div class="flex-none lg:flex mt-3 lg:mt-10">
                <div class="w-full lg:w-1/2 pr-0 lg:pr-16 xl:pr-28 2xl:pr-24">
                    <p class="font-bold">
                        Before getting started with the process of installing
                        and setting up the calling integration system within
                        your ERPNext - you need to abide by the following:
                    </p>
                    <p
                        class="text-base md:text-lg lg:text-xl font-bold mt-5 lg:mt-10"
                    >
                        Pre-requisites
                    </p>
                    <ul
                        class="list-disc ms-5 lg:ms-10 mt-3 text-[#31393C] text-xs md:text-sm lg:text-base"
                    >
                        <li class="font-semibold">Indiamart URL</li>
                        <a
                            class="text-[#3E96F4]"
                            href="https://mapi.indiamart.com/wservce/crm/crmListing/v2/"
                            >https://mapi.indiamart.com/wservce/crm/crmListing/v2/</a
                        >
                        <li class="font-semibold mt-2">
                            Indiamart Authorization Key:
                        </li>
                        <p class="font-semibold">
                            Hereâ€™s how you to get your authorization key:
                        </p>
                        <p>Indiamart > lead manager > Menu > CRM integration</p>
                        <Image
                            loading="eager"
                            src={indiamart_key_img}
                            alt="indiamart_key_img"
                            class="h-[134px] w-[198px] mt-2 cursor-pointer"
                            id="popup-trigger-one"
                        />
                        <div class="popup-container" id="popup-container-one">
                            <div class="popup-content">
                                <span
                                    class="close-btn"
                                    onclick="closePopup('popup-container-one')"
                                                                        >&times;</span
                                >
                                <Image
                                    loading="eager"
                                    src={indiamart_key}
                                    alt="indiamart_key"
                                    class=""
                                />
                            </div>
                        </div>

                        <p class="mt-2">Generate Authorization Key</p>

                        <Image
                            loading="eager"
                            src={authorization_key_img}
                            alt="authorization_key_img"
                            class="h-[96px] w-[198px] mt-2 cursor-pointer"
                            id="popup-trigger-two"
                        />
                        <div class="popup-container" id="popup-container-two">
                            <div class="popup-content">
                                <span
                                    class="close-btn"
                                    onclick="closePopup('popup-container-two')"
                                                                        >&times;</span
                                >
                                <Image
                                    loading="eager"
                                    src={authorization_key}
                                    alt="authorization_key"
                                    class=""
                                />
                            </div>
                        </div>
                        <li class="mt-2 font-semibold">
                            Registered Mobile Number with Indiamart
                        </li>
                        <p>
                            At the time of Indiamart registration, the mobile
                            number number which you have added, the same will be
                            added here
                        </p>
                    </ul>
                </div>

                <div class="w-full lg:w-1/2 mt-10 lg:mt-0 border-mx-10">
                    <!-- <div class="w-full lg:w-1/2 mt-10 lg:mt-0 border-l-2 border-dashed border-[#4D5F66] border-opacity-40"></div> -->
                    <div
                        class="flex gap-4 md:gap-5 lg:gap-6 border-l-2 border-dashed border-[#4D5F66] border-opacity-40 pb-[1.75rem] lg:pb-[1rem] xl:pb-[1.5rem] 2xl:pb-[1.75rem]"
                    >
                        <div class="">
                            <div
                                class="-ml-[0.55rem] xl:-ml-[0.60rem] w-4 lg:w-[1.125rem] h-4 lg:h-[1.125rem] bg-[#3E96F4] rounded-full flex items-center justify-center text-white border border-solid border-[#4D5F66]"
                            >
                            </div>
                        </div>
                        <div class="">
                            <p
                                class="text-xs md:text-sm lg:text-base leading-6 md:leading-6 lg:leading-7 -mt-1 md:-mt-[0.35rem] font-semibold text-[#31393C]"
                            >
                                Step 1:
                            </p>
                            <p
                                class="mb-1 md:mb-2 lg:mb-1 xl:mb-2 2xl:mb-2 item-center text-base md:text-lg lg:text-xl leading-6 md:leading-6 lg:leading-7 font-semibold text-[#31393C"
                            >
                                Installing the App:
                            </p>
                            <p
                                class="text-xs md:text-sm lg:text-base leading-5 md:leading-5 lg:leading-6 font-normal text-[#31393C]"
                            >
                                You need to install our Indimart integration on
                                your Bench or Site.
                            </p>
                            <p
                                class="ultimate_card_blue p-1 md:p-2 text-xs md:text-sm lg:text-base leading-5 md:leading-5 lg:leading-6 font-normal text-[#31393C] mt-3 border-[#31393C]"
                            >
                                Go to your bench or site, select app tabs > Add
                                app > Search the app(Indimart integration by
                                Sanskar Technolab)
                            </p>

                            <p
                                class="text-xs md:text-sm lg:text-base leading-5 md:leading-5 lg:leading-6 font-normal text-[#31393C] mt-3"
                            >
                                You can choose our app and install it easily
                                (after making the payment of $12).
                            </p>
                        </div>
                    </div>

                    <div
                        class="flex gap-4 md:gap-5 lg:gap-6 border-l-2 border-dashed border-[#4D5F66] border-opacity-40 pb-[1.75rem] lg:pb-[1rem] xl:pb-[1.5rem] 2xl:pb-[1.75rem]"
                    >
                        <div class="">
                            <div
                                class="-ml-[0.55rem] xl:-ml-[0.60rem] w-4 lg:w-[1.125rem] h-4 lg:h-[1.125rem] bg-[#3E96F4] rounded-full flex items-center justify-center text-white border border-solid border-[#4D5F66]"
                            >
                            </div>
                        </div>
                        <div class="">
                            <p
                                class="text-xs md:text-sm lg:text-base leading-6 md:leading-6 lg:leading-7 -mt-1 md:-mt-[0.35rem] font-semibold text-[#31393C]"
                            >
                                Step 2:
                            </p>
                            <p
                                class="mb-1 md:mb-2 lg:mb-1 xl:mb-2 2xl:mb-2 item-center text-base md:text-lg lg:text-xl leading-6 md:leading-6 lg:leading-7 font-semibold text-[#31393C]"
                            >
                                Enabling Indimart settings:
                            </p>
                            <p
                                class="text-xs md:text-sm lg:text-base leading-5 md:leading-5 lg:leading-6 font-normal text-[#31393C]"
                            >
                                After installing, you will be redirected to the
                                Indiamart workspace.
                            </p>
                            <p
                                class="ultimate_card_blue p-1 md:p-2 text-xs md:text-sm lg:text-base leading-5 md:leading-5 lg:leading-6 font-normal text-[#31393C] mt-3 border-[#31393C]"
                            >
                                Go to the Indiamart settings and click to enable
                                it.
                            </p>
                        </div>
                    </div>

                    <div
                        class="flex gap-4 md:gap-5 lg:gap-6 border-l-2 border-dashed border-[#4D5F66] border-opacity-40 pb-[1.75rem] lg:pb-[1rem] xl:pb-[1.5rem] 2xl:pb-[1.75rem]"
                    >
                        <div class="flex-shrink-0">
                            <div
                                class="-ml-[0.55rem] xl:-ml-[0.60rem] w-4 lg:w-[1.125rem] h-4 lg:h-[1.125rem] bg-[#3E96F4] rounded-full flex items-center justify-center text-white border border-solid border-[#4D5F66]"
                            >
                            </div>
                        </div>
                        <div class="">
                            <p
                                class="item-center text-xs md:text-sm lg:text-base leading-6 md:leading-6 -mt-1 md:-mt-[0.35rem] lg:leading-7 font-semibold text-[#31393C]"
                            >
                                Step 3:
                            </p>
                            <p
                                class="mb-1 md:mb-2 lg:mb-1 xl:mb-2 2xl:mb-2 item-center text-base md:text-lg lg:text-xl leading-6 md:leading-6 lg:leading-7 font-semibold text-[#31393C]"
                            >
                                Adding the required details:
                            </p>
                            <p
                                class="text-xs md:text-sm lg:text-base leading-5 md:leading-5 lg:leading-6 font-normal text-[#31393C]"
                            >
                                After enabling, one form will appear, you need
                                to add the Indiamart URL, Authorization Key, and
                                registered mobile number with Indiamart.
                            </p>
                            <p
                                class="ultimate_card_yellow p-1 md:p-2 text-xs md:text-sm lg:text-base leading-5 md:leading-5 lg:leading-6 font-normal text-[#31393C] mt-3 border-[#31393C]"
                            >
                                (Check the prerequisites for seamless set-up)
                            </p>
                        </div>
                    </div>
                    <div
                        class="flex gap-4 md:gap-5 lg:gap-6 border-l-2 border-dashed border-[#4D5F66] border-opacity-40 pb-[1.75rem] lg:pb-[1rem] xl:pb-[1.5rem] 2xl:pb-[1.75rem]"
                    >
